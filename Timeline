public class Timeline{
	tuit[] tweets;


	public Timeline(){
		this.tweets= new tuit[11];
		for (int i=0; i<this.tweets.length;i++){
			this.tweets[i]=new tuit();
		}

	}

	public void asignar(int posicion, String usuario, String texto, int RT){
		this.tweets[posicion].setTweet(usuario,texto,RT);
		

	}




	public void imprimirTweets(){
		for (int i=0; i<this.tweets.length;i++){
			System.out.println(this.tweets[i]);
		}
	}




	public static void main(String[] args){
		
		Timeline pacos= new Timeline();
		pacos.asignar(0, "Yo ",  "funciona porfis ", 4);
		pacos.imprimirTweets();

	}



}
